<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Database" namespace="JustTrade.Database">

 <class name="Permition" table="permitions">
  <id name="PermitionId" column="permition_id">
      <generator class="guid" />
    </id>
    <property name="Name" column="name"  type="String" />

    <many-to-one name="UserByPermition" class="User" column="user_id" not-null="true" insert="false" update="false" cascade="save-update"></many-to-one>

 </class>

</hibernate-mapping>
